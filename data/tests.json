    {
      "name": "Solo Dance",
      "displayName": "Solo Dance (Pattern Dance – Solo)",
      "levels": [
        {
          "level": "Preliminary",
          "tests": [
            {
              "name": "Solo Preliminary Dance Test",
              "elements": [
                "Solo Dutch Waltz",
                "Solo Canasta Tango",
                "Solo Rhythm Blues"
              ],
              "notes": "Same dance patterns as partnered, skated solo.",
              "sources": [
                "https://www.usfigureskating.org/test-requirements"
              ]
            }
          ]
        }
      ]
    },

    {
      "name": "Free Dance",
      "displayName": "Free Dance (Partnered & Solo)",
      "levels": [
        {
          "level": "Bronze",
          "tests": [
            {
              "name": "Solo Free Dance – Bronze",
              "elements": [
                "Duration: 1:30 ± 10 sec",
                "1 short edge element (3–7 sec)",
                "1 solo dance spin (≥3 rev; no flying entry)",
                "1 step sequence (circular/midline/diagonal; include 3-turns & mohawks)",
                "1 twizzle (not a series; ≥1 rev)"
              ],
              "notes": "Solo FD element definitions per current Rulebook.",
              "sources": [
                "https://sunshinestatefsc.com/wp-content/uploads/2024/09/Solo-Free-Dance-Test-Requirements.pdf",
                "https://www.usfigureskating.org/test-requirements"
              ]
            }
          ]
        },

        {
          "level": "Silver",
          "tests": [
            {
              "name": "Solo Free Dance – Silver",
              "elements": [
                "Duration: 2:20 ± 10 sec",
                "2 short edge elements (each 3–7 sec; different positions)",
                "1 solo dance spin (≥3 rev; no flying entry)",
                "1 step sequence (circular/midline/diagonal; include 3-turns, mohawks, brackets)",
                "1 twizzle series (≥1 rev on each foot; different entry edge/direction)"
              ],
              "sources": [
                "https://sunshinestatefsc.com/wp-content/uploads/2024/09/Solo-Free-Dance-Test-Requirements.pdf",
                "https://www.usfigureskating.org/test-requirements"
              ]
            }
          ]
        },

        {
          "level": "Gold",
          "tests": [
            {
              "name": "Solo Free Dance – Gold",
              "elements": [
                "Duration: 3:10 ± 10 sec",
                "Combo edge element + short edge element OR 3 short edge elements",
                "Step seq: include rockers, choctaws, twizzles",
                "Spins: no flying entries"
              ],
              "sources": [
                "https://sunshinestatefsc.com/wp-content/uploads/2024/09/Solo-Free-Dance-Test-Requirements.pdf",
                "https://www.usfigureskating.org/test-requirements"
              ]
            }
          ]
        }
      ]
    }
  ]
