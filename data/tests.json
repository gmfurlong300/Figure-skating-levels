    {
  "name": "Free Skate",
  "displayName": "Free Skate",
  "levels": [
    {
      "level": "Pre-Preliminary",
      "tests": [
        {
          "name": "Pre-Preliminary Free Skate",
          "elements": [
            "Program duration: 1:40 ± 10 seconds",
            "Jumps: Waltz jump, half jumps, single jumps (no axel required)",
            "Spins: Two spins in any position (minimum 3 revolutions each)",
            "Step sequence: One step sequence using full ice",
            "Restrictions: No axel or double jumps permitted"
          ],
          "notes": "Introductory free skate program demonstrating basic jump/spin control.",
          "sources": [
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Preliminary",
      "tests": [
        {
          "name": "Preliminary Free Skate",
          "elements": [
            "Program duration: 1:40 ± 10 seconds",
            "Jumps: Single jumps allowed; axel optional",
            "Combinations: One jump combination or sequence",
            "Spins: Two spins (minimum 3 revolutions), one may be a combination spin",
            "Step sequence: One choreographic step sequence",
            "Restrictions: No double jumps permitted"
          ],
          "notes": "Focus on clean single jumps and basic spin variety.",
          "sources": [
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Pre-Bronze",
      "tests": [
        {
          "name": "Pre-Bronze Free Skate",
          "elements": [
            "Program duration: 1:50 ± 10 seconds",
            "Jumps: All single jumps permitted, including axel",
            "Combinations: One jump combination and one jump sequence",
            "Spins: Two spins (minimum 4 revolutions), one may be a combination spin",
            "Step sequence: One choreographic step sequence",
            "Restrictions: No double jumps permitted"
          ],
          "notes": "Axel is allowed but not required; quality and control emphasized.",
          "sources": [
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Bronze",
      "tests": [
        {
          "name": "Bronze Free Skate",
          "elements": [
            "Program duration: 2:00 ± 10 seconds",
            "Jumps: All single jumps permitted, including axel",
            "Combinations: Up to two jump combinations or sequences",
            "Spins: Three spins (minimum 4 revolutions), one must be a combination spin",
            "Step sequence: One choreographic step sequence",
            "Restrictions: No double jumps permitted"
          ],
          "notes": "Highest level before double jumps are introduced.",
          "sources": [
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Pre-Silver",
      "tests": [
        {
          "name": "Pre-Silver Free Skate",
          "elements": [
            "Program duration: 2:10 ± 10 seconds",
            "Jumps: Single jumps and axel required; double salchow and double toe loop permitted",
            "Combinations: Up to two jump combinations or sequences",
            "Spins: Three spins (minimum 4 revolutions), one must be a combination spin",
            "Step sequence: One choreographic step sequence",
            "Restrictions: No double jumps other than 2S or 2T"
          ],
          "notes": "First level where double jumps may appear.",
          "sources": [
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Silver",
      "tests": [
        {
          "name": "Silver Free Skate",
          "elements": [
            "Program duration: 2:20 ± 10 seconds",
            "Jumps: All single jumps and axel required; double jumps permitted except 2A",
            "Combinations: Up to two jump combinations or sequences",
            "Spins: Three spins (minimum 4 revolutions), one must be a combination spin",
            "Step sequence: One choreographic step sequence",
            "Restrictions: No double axel or triples"
          ],
          "notes": "Introduces more advanced double jumps and spin difficulty.",
          "sources": [
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Pre-Gold",
      "tests": [
        {
          "name": "Pre-Gold Free Skate",
          "elements": [
            "Program duration: 2:30 ± 10 seconds",
            "Jumps: All single jumps and double jumps permitted except 2A optional",
            "Combinations: Up to two jump combinations or sequences",
            "Spins: Three spins with increased difficulty (minimum 5 revolutions)",
            "Step sequence: One choreographic step sequence",
            "Restrictions: No triple jumps"
          ],
          "notes": "High-level double jump proficiency expected.",
          "sources": [
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Gold",
      "tests": [
        {
          "name": "Gold Free Skate",
          "elements": [
            "Program duration: 3:00 ± 10 seconds",
            "Jumps: All single and double jumps permitted; double axel optional",
            "Combinations: Up to two jump combinations or sequences",
            "Spins: Three spins with advanced positions (minimum 5 revolutions)",
            "Step sequence: One choreographic step sequence",
            "Restrictions: No triple jumps"
          ],
          "notes": "Highest level of Free Skate testing; strong skating skills and double jumps required.",
          "sources": [
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    }
  ]
},

    {
  "name": "Free Dance",
  "displayName": "Free Dance",
  "levels": [
    {
      "level": "Bronze",
      "tests": [
        {
          "name": "Solo Free Dance – Bronze",
          "elements": [
            "Program duration: 1:30 ± 10 seconds",
            "1 short edge element (3–7 seconds)",
            "1 solo dance spin (minimum 3 revolutions; no flying entry)",
            "1 step sequence (circular, midline, or diagonal; must include 3-turns and mohawks)",
            "1 twizzle (not a series; minimum 1 revolution)"
          ],
          "notes": "Introductory level of Solo Free Dance. Focus on musicality, basic edge quality, and clean execution of required elements.",
          "sources": [
            "https://sunshinestatefsc.com/wp-content/uploads/2024/09/Solo-Free-Dance-Test-Requirements.pdf",
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Pre-Silver",
      "tests": [
        {
          "name": "Solo Free Dance – Pre-Silver",
          "elements": [
            "Program duration: 2:00 ± 10 seconds",
            "1 short edge element (3–7 seconds)",
            "1 solo dance spin (minimum 3 revolutions; no flying entry)",
            "1 step sequence (circular, midline, or diagonal; must include 3-turns, mohawks, and brackets)",
            "1 twizzle series (minimum 1 revolution on each foot; different entry edge or direction)"
          ],
          "notes": "Introduces twizzle series and more complex step sequence requirements.",
          "sources": [
            "https://sunshinestatefsc.com/wp-content/uploads/2024/09/Solo-Free-Dance-Test-Requirements.pdf",
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Silver",
      "tests": [
        {
          "name": "Solo Free Dance – Silver",
          "elements": [
            "Program duration: 2:20 ± 10 seconds",
            "2 short edge elements (each 3–7 seconds; must be in different positions)",
            "1 solo dance spin (minimum 3 revolutions; no flying entry)",
            "1 step sequence (circular, midline, or diagonal; must include 3-turns, mohawks, and brackets)",
            "1 twizzle series (minimum 1 revolution on each foot; different entry edge or direction)"
          ],
          "notes": "Requires two short edge elements and more advanced turn variety in the step sequence.",
          "sources": [
            "https://sunshinestatefsc.com/wp-content/uploads/2024/09/Solo-Free-Dance-Test-Requirements.pdf",
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Pre-Gold",
      "tests": [
        {
          "name": "Solo Free Dance – Pre-Gold",
          "elements": [
            "Program duration: 2:40 ± 10 seconds",
            "Combo edge element + short edge element OR 3 short edge elements (each 3–7 seconds; combo ≤12 seconds total)",
            "1 solo dance spin (minimum 3 revolutions; no flying entry)",
            "1 step sequence including rockers, counters, and choctaws",
            "1 twizzle series (multi-directional or multi-entry)"
          ],
          "notes": "First level requiring rockers, counters, and choctaws in the step sequence.",
          "sources": [
            "https://sunshinestatefsc.com/wp-content/uploads/2024/09/Solo-Free-Dance-Test-Requirements.pdf",
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    },
    {
      "level": "Gold",
      "tests": [
        {
          "name": "Solo Free Dance – Gold",
          "elements": [
            "Program duration: 3:10 ± 10 seconds",
            "Combo edge element + short edge element OR 3 short edge elements (3–7 seconds each; combo ≤12 seconds total)",
            "1 solo dance spin (minimum 3 revolutions; no flying entry)",
            "1 step sequence including rockers, choctaws, and twizzles",
            "1 twizzle series (multi-rev, multi-directional, or complex entry)"
          ],
          "notes": "Highest level of Solo Free Dance. Requires mastery of advanced turns, musical interpretation, and strong edge control.",
          "sources": [
            "https://sunshinestatefsc.com/wp-content/uploads/2024/09/Solo-Free-Dance-Test-Requirements.pdf",
            "https://www.usfigureskating.org/test-requirements"
          ]
        }
      ]
    }
  ]
},

